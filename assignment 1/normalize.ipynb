    "def norm(m):\n",
    "   row=m.shape[0]\n",
    "   col=m.shape[1]\n",
    "   sum=0\n",
    "   for i in range(0,row):\n",
    "     for j in range(0,col):\n",
    "        sum=sum+m[i][j]\n",
    "     print(sum)\n",
    "     for j in range(0,col):\n",
    "        m[i][j]=m[i][j]/sum        \n",
    "   return m"
   ]
